Ext.onReady(function(){
    if(Ext.Updater){
        Ext.Updater.defaults.indicatorText = '<div class="loading-indicator">–ó–∞–≤–∞–Ω—Ç–∞–∂–µ–Ω–Ω—è...</div>';
    }
    if(Ext.view.View){
       Ext.view.View.prototype.emptyText = "<–ü–æ—Ä–æ–∂–Ω—å–æ>";
    }

    if(Ext.grid.Panel){
       Ext.grid.Panel.prototype.ddText = "{0} –æ–±—Ä–∞–Ω–∏—Ö —Ä—è–¥–∫—ñ–≤";
    }

    if(Ext.TabPanelItem){
       Ext.TabPanelItem.prototype.closeText = "–ó–∞–∫—Ä–∏—Ç–∏ —Ü—é –≤–∫–ª–∞–¥–∫—É";
    }

    if(Ext.form.field.Base){
       Ext.form.field.Base.prototype.invalidText = "–•–∏–±–Ω–µ –∑–Ω–∞—á–µ–Ω–Ω—è";
    }

    if(Ext.LoadMask){
       Ext.LoadMask.prototype.msg = "–ó–∞–≤–∞–Ω—Ç–∞–∂–µ–Ω–Ω—è...";
    }

    if(Ext.Date) {
        Ext.Date.monthNames = [
           "–°—ñ—á–µ–Ω—å",
           "–õ—é—Ç–∏–π",
           "–ë–µ—Ä–µ–∑–µ–Ω—å",
           "–ö–≤—ñ—Ç–µ–Ω—å",
           "–¢—Ä–∞–≤–µ–Ω—å",
           "–ß–µ—Ä–≤–µ–Ω—å",
           "–õ–∏–ø–µ–Ω—å",
           "–°–µ—Ä–ø–µ–Ω—å",
           "–í–µ—Ä–µ—Å–µ–Ω—å",
           "–ñ–æ–≤—Ç–µ–Ω—å",
           "–õ–∏—Å—Ç–æ–ø–∞–¥",
           "–ì—Ä—É–¥–µ–Ω—å"
        ];

        Ext.Date.dayNames = [
           "–ù–µ–¥—ñ–ª—è",
           "–ü–æ–Ω–µ–¥—ñ–ª–æ–∫",
           "–í—ñ–≤—Ç–æ—Ä–æ–∫",
           "–°–µ—Ä–µ–¥–∞",
           "–ß–µ—Ç–≤–µ—Ä",
           "–ü‚Äô—è—Ç–Ω–∏—Ü—è",
           "–°—É–±–æ—Ç–∞"
        ];
