    }

    if(Ext.form.field.VTypes){
       Ext.apply(Ext.form.field.VTypes, {
          emailText    : '–¶–µ –ø–æ–ª–µ –ø–æ–≤–∏–Ω–Ω–æ –º—ñ—Å—Ç–∏—Ç–∏ –∞–¥—Ä–µ—Å—É –µ–ª–µ–∫—Ç—Ä–æ–Ω–Ω–æ—ó –ø–æ—à—Ç–∏ —É —Ñ–æ—Ä–º–∞—Ç—ñ "user@example.com"',
          urlText      : '–¶–µ –ø–æ–ª–µ –ø–æ–≤–∏–Ω–Ω–æ –º—ñ—Å—Ç–∏—Ç–∏ URL —É —Ñ–æ—Ä–º–∞—Ç—ñ "http:/'+'/www.example.com"',
          alphaText    : '–¶–µ –ø–æ–ª–µ –ø–æ–≤–∏–Ω–Ω–æ –º—ñ—Å—Ç–∏—Ç–∏ –≤–∏–∫–ª—é—á–Ω–æ –ª–∞—Ç–∏–Ω—Å—å–∫—ñ –ª—ñ—Ç–µ—Ä–∏ —Ç–∞ —Å–∏–º–≤–æ–ª –ø—ñ–¥–∫—Ä–µ—Å–ª–µ–Ω–Ω—è "_"',
          alphanumText : '–¶–µ –ø–æ–ª–µ –ø–æ–≤–∏–Ω–Ω–æ –º—ñ—Å—Ç–∏—Ç–∏ –≤–∏–∫–ª—é—á–Ω–æ –ª–∞—Ç–∏–Ω—Å—å–∫—ñ –ª—ñ—Ç–µ—Ä–∏, —Ü–∏—Ñ—Ä–∏ —Ç–∞ —Å–∏–º–≤–æ–ª –ø—ñ–¥–∫—Ä–µ—Å–ª–µ–Ω–Ω—è "_"'
       });
    }

    if(Ext.form.field.HtmlEditor){
       Ext.apply(Ext.form.field.HtmlEditor.prototype, {
         createLinkText : '–ë—É–¥—å-–ª–∞—Å–∫–∞ –≤–≤–µ–¥—ñ—Ç—å –∞–¥—Ä–µ—Å—É:',
         buttonTips : {
                bold : {
                   title: '–ù–∞–ø—ñ–≤–∂–∏—Ä–Ω–∏–π (Ctrl+B)',
                   text: '–ó—Ä–æ–±–∏—Ç–∏ –Ω–∞–ø—ñ–≤–∂–∏—Ä–Ω–∏–º –≤–∏–¥—ñ–ª–µ–Ω–∏–π —Ç–µ–∫—Å—Ç.',
                   cls: Ext.baseCSSPrefix + 'html-editor-tip'
                },
                italic : {
                   title: '–ö—É—Ä—Å–∏–≤ (Ctrl+I)',
                   text: '–ó—Ä–æ–±–∏—Ç–∏ –∫—É—Ä—Å–∏–≤–æ–º –≤–∏–¥—ñ–ª–µ–Ω–∏–π —Ç–µ–∫—Å—Ç.',
                   cls: Ext.baseCSSPrefix + 'html-editor-tip'
                },
                underline : {
                   title: '–ü—ñ–¥–∫—Ä–µ—Å–ª–µ–Ω–∏–π (Ctrl+U)',
                   text: '–ó—Ä–æ–±–∏—Ç–∏ –ø—ñ–¥–∫—Ä–µ—Å–ª–µ–Ω–∏–º –≤–∏–¥—ñ–ª–µ–Ω–∏–π —Ç–µ–∫—Å—Ç.',
                   cls: Ext.baseCSSPrefix + 'html-editor-tip'
               },
               increasefontsize : {
                   title: '–ó–±—ñ–ª—å—à–∏—Ç–∏ —Ä–æ–∑–º—ñ—Ä',
